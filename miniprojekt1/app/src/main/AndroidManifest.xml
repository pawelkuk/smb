<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pjatk.pawelkuklinski.miniprojekt1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Miniprojekt1">
        <activity android:name=".OptionsActivity" />
        <activity android:name=".EditProductsActivity">
            <intent-filter>
                <action android:name="com.pjatk.pawelkuklinski.miniproject2.EDIT_PRODUCT" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductsActivity">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>